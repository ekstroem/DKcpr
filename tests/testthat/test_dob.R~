context("Check that correct date-of-birth is computed")

cpr <- c("1010101234",
         "1010102234",
         "1010103234",
         "1010364234",
         "1010374234",
         "1010575234",
         "1010585234",
         "1010576234",
         "1010586234",
         "1010577234",
         "1010587234",
         "1010578234",
         "1010588234",
         "1010369234",
         "1010379234"         
         )
truedob <- as.Date(c("1910-10-10",
                     "1910-10-10",
                     "1910-10-10",
                     "2036-10-10",
                     "1937-10-10",
                     "2057-10-10",
                     "1858-10-10",
                     "2057-10-10",
                     "1858-10-10",
                     "2057-10-10",
                     "1858-10-10",
                     "2057-10-10",
                     "1858-10-10",
                     "2036-10-10",
                     "1937-10-10")

test_that("Birth year works", {
          expect_equal(date_of_birth(cpr), truedob)
})
